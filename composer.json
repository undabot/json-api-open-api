    {
      "type": "gitlab",
      "url": "git@gitlab.com:undabot/json-api-core.git"
    }
  ],
  "require": {
    "undabot/json-api-core": "^1.0.0",
    "undabot/json-api-symfony": "^1.0.0",
    "doctrine/annotations": "^1.6",
    "symfony/yaml": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "JsonApiOpenApi\\": "src/",
      "Example\\": "example/"
